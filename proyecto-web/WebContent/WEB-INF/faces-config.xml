<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>autores</from-outcome>
			<to-view-id>/autores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reporte</from-outcome>
			<to-view-id>/reporte.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>autores.xhtml</display-name>
		<from-view-id>/autores.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>reporte.xhtml</display-name>
		<from-view-id>/reporte.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>reporte.xhtml</display-name>
		<from-view-id>/reporte.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>autores</from-outcome>
			<to-view-id>/autores.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>autores.xhtml</display-name>
		<from-view-id>/autores.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reporte</from-outcome>
			<to-view-id>/reporte.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
